<rdf:RDF
    xmlns="http://iosb.fraunhofer.de/testontology#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#">
  <owl:Ontology rdf:about="http://iosb.fraunhofer.de/testontology"/>
  <owl:Class rdf:about="http://iosb.fraunhofer.de/testontology#ClassA">
    <owl:disjointWith>
      <owl:Class rdf:about="http://iosb.fraunhofer.de/testontology#ClassB"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:ObjectProperty rdf:about="http://iosb.fraunhofer.de/testontology#aToB"/>
  <ClassA rdf:about="http://iosb.fraunhofer.de/testontology#i1">
    <aToB>
      <ClassB rdf:about="http://iosb.fraunhofer.de/testontology#i2">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
      </ClassB>
    </aToB>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
  </ClassA>
</rdf:RDF>
